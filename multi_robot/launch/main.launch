<launch>
  <param name="/use_sim_time" value="true" />

   <!-- start world empty_world.world--> 
  <node name="gazebo" pkg="gazebo_ros" type="gazebo" 
   args="$(find multi_robot)/worlds/empty_world.world" respawn="false" output="screen" />

  <!-- start gui -->
  <!-- <node name="gazebo_gui" pkg="gazebo_ros" type="gui" respawn="false" output="screen"/> -->

  <!-- include our robots -->
   <include file="$(find multi_robot)/launch/leader.launch"/>  
</launch>
